# Example environment configuration file
# Copy this file to .env and update with your actual paths

# Database root paths for different datasets
# DNS Challenge Dataset
DNS_DB_ROOT=/path/to/DNS-Challenge-16kHz
DNS_CLEAN_PATH=datasets/clean
DNS_NOISE_PATH=datasets/noise
DNS_RIR_PATH=datasets/impulse_responses

# RIR Paths (Room Impulse Response)
RIR_DNS_48K=/path/to/DNS_RIR_48k


# VCTK Dataset
VCTK_DB_ROOT=/path/to/VCTK
VCTK_ND_DB_ROOT=/path/to/VCTK_ND
VCTK_ND_CLEAN_PATH=data/VCTK_ND_E/clean
VCTK_ND_NOISY_PATH=data/VCTK_ND_E/noisy
VCTK_DEMAND_DB_ROOT=/path/to/VCTK_DEMAND
VCTK_CORPUS_DB_ROOT=/path/to/VCTK-Corpus-0.92

# UNIVERSE Dataset
UNIVERSE_DB_ROOT=/path/to/UNIVERSE

# VoxCeleb Dataset
VOXCELEB_SAMPLE_DB_ROOT=/path/to/VoxCeleb_sample_50_SE

# Output SCP file root
SCP_ROOT=data/scp

# CUDA Configuration (optional, for local CUDA installation)
# Uncomment and update the path if you have a local CUDA installation
# CUDA_HOME=/path/to/cuda-12.4
